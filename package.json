{
  "name": "lively_mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "ios": "react-native run-ios --scheme dev_lively_mobile",
    "ios:staging": "react-native run-ios --scheme staging_lively_mobile",
    "ios:prod": "react-native run-ios --scheme prod_lively_mobile",
    "android": "react-native run-android --variant=devDebug --appIdSuffix dev",
    "android-release-variant": "react-native run-android --variant=devRelease --appIdSuffix dev",
    "android-release": "cd android && ./gradlew clean && ./gradlew assembleDevRelease",
    "android:staging": "react-native run-android --variant=stagingDebug --appIdSuffix staging",
    "android:staging-release-variant": "react-native run-android --variant=stagingRelease --appIdSuffix staging",
    "android:staging-release": "cd android && ./gradlew clean && ./gradlew assembleStagingRelease",
    "android:prod": "react-native run-android --variant=prodDebug --appIdSuffix prod",
    "android:prod-release-variant": "react-native run-android --variant=prodRelease --appIdSuffix prod",
    "android:prod-release": "cd android && ./gradlew clean && ./gradlew bundleRelease",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "@ptomasroos/react-native-multi-slider": "^2.2.2",
    "@react-native-camera-roll/camera-roll": "^5.7.2",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/blur": "^4.3.2",
    "@react-native-masked-view/masked-view": "^0.2.9",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@realm/react": "^0.4.3",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-native-vector-icons": "^6.4.13",
    "deprecated-react-native-prop-types": "^4.1.0",
    "mixpanel-react-native": "^2.3.1",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-hook-form": "^7.43.9",
    "react-native": "0.71.6",
    "react-native-config": "^1.5.1",
    "react-native-date-picker": "^4.2.13",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-fs": "^2.20.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-image-picker": "^5.6.0",
    "react-native-linear-gradient": "^2.7.3",
    "react-native-otp-verify": "^1.1.4",
    "react-native-pell-rich-editor": "^1.9.0",
    "react-native-qrcode-svg": "^6.2.0",
    "react-native-safe-area-context": "^4.5.1",
    "react-native-screens": "^3.20.0",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-splash-screen": "^3.3.0",
    "react-native-svg": "^13.9.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-webview": "^12.0.2",
    "react-redux": "^8.0.5",
    "realm": "^11.7.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-native-cli": "^2.0.1",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
